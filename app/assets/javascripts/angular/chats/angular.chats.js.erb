var chatsModule = angular.module('chatsModule', ['ngResource', 'ngAnimate', 'luegg.directives', 'timer']);

chatsModule
  .config(function($httpProvider){
		$httpProvider.defaults.transformRequest = function(data)
		{
			if (data === undefined) {
				return data;
			}
			var result = JSON.stringify(data);
			return result;
		}
});