	<%= form_for @user, url: { controller: "users", action: "create" } do |f| %>
	
		<%= f.text_field :email, :type => "email", :placeholder => "john@example.com" %>
		<%= f.text_field :zipcode, :placeholder => "Your zipcode" %>
		<%= f.text_field :promo_code, :placeholder => "Got a promo code?"%>
		<%= f.submit "Get Started", :class => "submit_button_white button" %>
	<% end %>